<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/SETTINGS/1.2.0"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd">

    <localRepository>/tmp/maven-proxy/.m2</localRepository>

    <interactiveMode>true</interactiveMode>
    <offline>false</offline>

    <mirrors>
        <mirror>
            <id>central</id>
            <name>local-maven-proxy</name>
            <url>http://localhost:7999</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>

</settings>
